<Window x:Class="Burdator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Burdator"
        mc:Ignorable="d"
        Title="MainWindow" Height="470" Width="800">
    <Grid>
        <StackPanel>
            <Button FontSize = "35" Content="Калькулятор" HorizontalAlignment="Center" Margin="0,70,0,30" VerticalAlignment="Top" Width="474" Height="60" Click="Button_Click_2"/>
            <Button FontSize = "35" Content="Графики" HorizontalAlignment="Center" Margin="0,0,0,30" VerticalAlignment="Top" Width="474" Height="60" Click="Button_Click_3"/>
            <Button FontSize = "35" x:Name="squareButton" Content="Квадратное уравнение" Margin="0,0,0,30" HorizontalAlignment="Center" VerticalAlignment="Top" Width="474" Height="60" Click="Button_Click"/>
            <Button FontSize = "35"  Content="Логарифмы" HorizontalAlignment="Center" Margin="0,0,0,30" VerticalAlignment="Top" Width="474" Height="60" Click="Button_Click_1"/>
        </StackPanel>
    </Grid>
</Window>
